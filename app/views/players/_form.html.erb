<%=form_for(@player) do |f| %>
	<p>
		<%=f.label :name%><br>
		<%=f.text_field :name%><br>
	</p>
	<p>
		<%=f.label :team_id%><br>
		<%=f.text_field :team_id%><br>
	</p>
	<p>
		<%=f.label :position%><br>
		<%=f.text_field :position%><br>
	</p>
	<p>
		<%=f.label :number%><br>
		<%=f.text_field :number%><br>
	</p>
	<%=f.submit%>
<%end%>